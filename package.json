{
  "name": "js-tic-tac-toe-testing",
  "version": "1.0.0",
  "description": "The purpose of this project is to build the TIC-TAC-TOE game to play on the browser, it is required for this project using Factory functions and module  pattern using javascript.",
  "private": true,
  "scripts": {
    "build": "webpack",
    "buildwatch": "webpack --config webpack.config.js --watch",
    "test": "jest",
    "watchtest": "jest --watch *.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiguelDP4/js-tic-tac-toe-testing.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MiguelDP4/js-tic-tac-toe-testing/issues"
  },
  "homepage": "https://github.com/MiguelDP4/js-tic-tac-toe-testing#readme",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-jest": "^26.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^26.1.0",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-csstree-validator": "^1.8.0",
    "stylelint-scss": "^3.17.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
